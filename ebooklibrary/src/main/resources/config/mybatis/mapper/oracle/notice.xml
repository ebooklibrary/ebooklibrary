<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mybatis.mapper.oracle.ebooklibraryNotice">
		
	<insert id="insertNotice" parameterType="NoticeVO">
		<selectKey order="BEFORE" keyProperty="no" resultType="int">
			select notice_seq.nextval from dual
		</selectKey>
			insert into notice_board(notice_no, title,writer ,content) 
			values (#{notice_no},#{title},#{writer},#{content})
	</insert>
	
<!-- 	<select id="" parameterType="NoticeVO" resultType="">
		select * from notice_board order by #{notice_no} desc
	</select> -->
	
</mapper>























