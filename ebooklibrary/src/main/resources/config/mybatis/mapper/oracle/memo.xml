<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="config.mybatis.mapper.oracle.memo">
	<select id="" parameterType="MemoVO" resultType="MemoVO">
		select * from memo where to_Id=#{toId}
	</select>
	
	<select id="selectMemoCount" parameterType="String" resultType="int">
		select count(*) from memo where to_Id=#{toId} and check='N'
	</select>
</mapper>























